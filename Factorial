// Factorial Number 

#include<stdio.h>
int main(){
    int i, n;
    long fact = 1;
    
    printf("Enter the number \n:");
    scanf("%d", &n);
    
    for(int i=1; i<=n; i++){
        fact *=i;
    }
    
    printf("Factorial number is %ld", fact);
    
    return 0;
}

// output
Enter the number 
:8  
Factorial number is 40320
