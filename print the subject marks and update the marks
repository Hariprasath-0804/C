//print the subject marks and update the marks
#include<stdio.h>

int marks[] = {75,80,85,90,95};

int main(){
    printf("tamil marks:%d \n",marks[4]);
    printf("english marks: %d \n", marks[1]);
    
    printf("print all the marks : \n" );
    
    for(int i=0; i<=4; i++){
        printf("%d \n" , marks[i]);
        
    }
    
    marks[2] = 40;
    
    printf("updated marks : \n");
    for(int i=0; i<5; i++){
        printf("%d \n", marks[i]);
    }
    return 0;
}
\\ ouput
tamil marks:95 
english marks: 80 
print all the marks : 
75 
80 
85 
90 
95 
updated marks : 
75 
80 
40 
90 
95 
