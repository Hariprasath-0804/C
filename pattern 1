//Line star pattern :
#include <stdio.h>

int i,j,n;

int main(){
    
    printf("enter your number : \n");
    scanf("%d",&n);
    
    for(int i=n;i>=n;i--)
    {
        for(int j=1;j<=n;j++)
        {
            printf(" ");
        }
        for(int j=1;j<=n;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    
    return 0;
}
// output
enter your number : 
8
        ********
