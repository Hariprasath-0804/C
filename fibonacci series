
//Fibonacci Series 


#include<stdio.h>

int main(){
    
    int n, t1=0, t2=1, next, i;
    
    printf("Enter the number \n");
    scanf("%d", &n);
    
    for(int i=1; i<=n; i++){
        printf("%d\n", t1);
        next=t1 + t2;
        t1 = t2;
        t2=next;
    }
    // printf("\n");
    return 0;
}
// output
Enter the number 
8
0
1
1
2
3
5
8
13
